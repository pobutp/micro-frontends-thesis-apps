{
  "root": "apps/horizontal-shell-react",
  "sourceRoot": "apps/horizontal-shell-react/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/horizontal-shell-react",
        "index": "apps/horizontal-shell-react/src/index.html",
        "baseHref": "/",
        "main": "apps/horizontal-shell-react/src/index.ts",
        "polyfills": "apps/horizontal-shell-react/src/polyfills.ts",
        "tsConfig": "apps/horizontal-shell-react/tsconfig.app.json",
        "assets": [
          "apps/horizontal-shell-react/src/favicon.ico",
          "apps/horizontal-shell-react/src/assets"
        ],
        "styles": ["apps/horizontal-shell-react/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/horizontal-shell-react/webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/horizontal-shell-react/src/environments/environment.ts",
              "with": "apps/horizontal-shell-react/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "horizontal-shell-react:build",
        "hmr": false
      },
      "configurations": {
        "production": {
          "buildTarget": "horizontal-shell-react:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/horizontal-shell-react/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/horizontal-shell-react"],
      "options": {
        "jestConfig": "apps/horizontal-shell-react/jest.config.js",
        "passWithNoTests": true
      }
    },
    "serve-mfe": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx serve horizontal-shell-react",
          "nx serve horizontal-basket-mfe",
          "nx serve horizontal-catalog-mfe"
        ]
      }
    }
  },
  "tags": []
}
